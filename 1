import numpy as np
x=np.random.rand(4,2)
W1=np.random.rand(3,4)
b1=np.random.rand(3,1)
W2=np.random.rand(5,3)
b2=np.random.rand(1,2)
def piecewise_linear(x):
    return np.maximum(0,x)
Z1=np.dot(W1,x)+b1
A1=piecewise_linear(Z1)
Z2=np.dot(W2,A1)+b2
A2=piecewise_linear(Z2)
print("Z1:\n", Z1)
print("A1:\n", A1)
print("Z2:\n", Z2)
print("A2:\n", A2)
